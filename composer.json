{
    "type": "library",
    "name": "sweetchuck/lint-report",
    "description": "Generate reports from lint results",
    "license": "GPL-3.0-or-later",
    "keywords": [
        "lint"
    ],
    "authors": [
        {
            "name": "Andor",
            "role": "Maintainer",
            "homepage": "https://github.com/Sweetchuck"
        }
    ],
    "homepage": "https://github.com/Sweetchuck/lint-report",
    "support": {
        "source": "https://github.com/Sweetchuck/lint-report",
        "issues": "https://github.com/Sweetchuck/lint-report/issues"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin",
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": ">=7.4",
        "ext-dom": "*",
        "ext-json": "*",
        "symfony/console": "^4.0 || ^5.0",
        "symfony/filesystem": "^4.0 || ^5.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.1",
        "codeception/module-filesystem": "^1.0",
        "consolidation/robo": "^3.0",
        "mikey179/vfsstream": "^1.6",
        "squizlabs/php_codesniffer": "^3.4",
        "stecman/symfony-console-completion": "^0.11",
        "sweetchuck/codeception-module-robo-task-runner": "2.x-dev",
        "sweetchuck/git-hooks": "2.x-dev",
        "sweetchuck/robo-git": "2.x-dev",
        "symfony/yaml": "^4.0 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Sweetchuck\\LintReport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sweetchuck\\LintReport\\Test\\": "tests/_support/",
            "Sweetchuck\\LintReport\\Tests\\Unit\\": "tests/unit/"
        }
    },
    "scripts": {},
    "scripts-descriptions": {}
}
